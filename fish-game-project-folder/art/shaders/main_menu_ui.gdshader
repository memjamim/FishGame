shader_type canvas_item;

uniform float height = 7.5;
uniform float speed = 1.0;
uniform float frequency = 25.0;

void vertex() {
	VERTEX.y += sin((frequency * UV.x) + (speed * TIME)) * height;
}

void fragment() {
	// Called for every pixel the material is visible on.
}

